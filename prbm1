class Solution {
    static String oddEven(int n) {
        //Scanner sc=new Scanner(System.in);
        //int n=sc.nextInt();
        if(n%2==0){
            return "even";
        }
        else{
            return "odd";
        }
    }
       public static void main(String args[]){
          Scanner sc=new Scanner(System.in);
           int n=sc.nextInt();
           System.out.println(oddEven( n));
       }
    
}
